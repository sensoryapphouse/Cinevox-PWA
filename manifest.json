{
  "name": "CineVox",
  "short_name": "CineVox",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "description": "change live camera view with sound",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": [{
    "src": "images/cinevox128.png",
      "sizes": "128x128",
      "type": "image/png"
    }, {
      "src": "images/cinevox144.png",
      "sizes": "144x144",
      "type": "image/png"
    }, {
      "src": "images/cinevox152.png",
      "sizes": "152x152",
      "type": "image/png"
    }, {
      "src": "images/cinevox192.png",
      "sizes": "192x192",
      "type": "image/png"
    }, {
      "src": "images/cinevox256.png",
      "sizes": "256x256",
      "type": "image/png"
    }, {
      "src": "images/cinevox512.png",
      "sizes": "512x512",
      "type": "image/png"
    }],
  "lang": "en-US",
  "start_url": "./index.html",
  "display": "standalone"
}
